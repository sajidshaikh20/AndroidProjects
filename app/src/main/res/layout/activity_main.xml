<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/instagrambtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="@string/Instagrambtn"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/Pelindromebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Pelindrome"
        app:layout_constraintStart_toStartOf="@id/instagrambtn"
        app:layout_constraintEnd_toEndOf="@+id/instagrambtn"
        app:layout_constraintTop_toBottomOf="@+id/instagrambtn"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/patternbtn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/Pelindromebtn"
        app:layout_constraintStart_toStartOf="@+id/Pelindromebtn"
        app:layout_constraintEnd_toEndOf="@+id/Pelindromebtn"
        android:text="Pattern"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/healthformbtn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/patternbtn"
        app:layout_constraintStart_toStartOf="@+id/patternbtn"
        app:layout_constraintEnd_toEndOf="@+id/patternbtn"
        android:padding="1dp"
        android:text="HealthForm"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/linkedlnbtn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/healthformbtn"
        app:layout_constraintStart_toStartOf="@+id/healthformbtn"
        app:layout_constraintEnd_toEndOf="@+id/healthformbtn"
        android:text="Linkedln"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/intentsbtn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:padding="1dp"
        app:layout_constraintTop_toBottomOf="@id/linkedlnbtn"
        app:layout_constraintStart_toStartOf="@+id/linkedlnbtn"
        app:layout_constraintEnd_toEndOf="@+id/linkedlnbtn"
        android:text="intent Actions"
        />
</androidx.constraintlayout.widget.ConstraintLayout>